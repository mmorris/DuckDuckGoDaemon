<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>com.mattmorris.duckduckgo</string>
	<key>ProgramArguments</key>
	<array>
		<string>/bin/bash</string>
		<string>-c</string>
		<string>if ! /usr/bin/egrep -q "search.yahoo.com" /etc/hosts; then echo "184.72.115.86 search.yahoo.com"; fi</string>
	</array>
	<key>StandardOutPath</key>
	<string>/etc/hosts</string>
	<key>WatchPaths</key>
	<array>
		<string>/etc/hosts</string>
	</array>
	<key>RunAtLoad</key>
	<true/>
</dict>
</plist>
